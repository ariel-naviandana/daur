{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "vercel-php@0.7.3",
            "config": {
                "installCommand": "composer install --no-dev --optimize-autoloader",
                "outputDirectory": "public"
            }
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "installCommand": "npm install",
                "buildCommand": "npm run build",
                "outputDirectory": "public"
            }
        }
    ],
    "routes": [
        { "src": "/js/(.*)", "dest": "/public/js/$1" },
        { "src": "/css/(.*)", "dest": "/public/css/$1" },
        { "src": "/dist/(.*)", "dest": "/public/dist/$1" },
        { "src": "/(.*)", "dest": "/public/index.php" }
    ],
    "env": {
        "APP_NAME": "Laravel",
        "APP_ENV": "production",
        "APP_KEY": "base64:hKzV7EcXOGwErHetAUsTdDoVFBO5OuYe2sEHR+22erU=",
        "APP_DEBUG": "false",
        "APP_URL": "https://your-app-url.vercel.app",
        "OPENCAGE_API_KEY": "e5f8659f848545f486b84de646bd104d",
        "APP_LOCALE": "en",
        "APP_FALLBACK_LOCALE": "en",
        "APP_FAKER_LOCALE": "en_US",
        "APP_MAINTENANCE_DRIVER": "file",
        "PHP_CLI_SERVER_WORKERS": "4",
        "BCRYPT_ROUNDS": "12",
        "LOG_CHANNEL": "errorlog",
        "LOG_STACK": "single",
        "LOG_DEPRECATIONS_CHANNEL": "null",
        "LOG_LEVEL": "info",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "n5mjf.h.filess.io",
        "DB_PORT": "61002",
